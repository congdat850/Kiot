<div class="list-warehouse-cover-surface">
    <h3 class="title-page">DANH SÁCH MẶT PHỦ</h3>
    <div class="search-component">
        <cd-search for="coveredSurface" categorys={{this.categorys}}></cd-search>
    </div>
    <table class="table-list">
        <tr>
            <th>STT</th>
            <th>Loại mặt phủ</th>
            <th>Mã màu</th>
            <th>Số lượng</th>
        </tr>
        {{#each coveredSurface}}
        <tr>
            <td>{{inc @index}}</td>
            <td>{{this.LoaiMatPhu}}</td>
            <td>{{this.MaMau}}</td>
            <td>{{this.SoLuong}}</td>
        </tr>
        {{/each}}
    </table>
    <hd-paging current-page="{{page}}" max-page="{{maxPage}}"></hd-paging>
</div>