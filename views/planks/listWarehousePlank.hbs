<link rel="stylesheet" href="/stylesheets/listWarehousePlank.css">
<table id="customers">
    <tr>
        <th>Mã ván</th>
        <th>Tên ván</th>
        <th>Độ dày</th>
        <th>Nhà cung cấp</th>
        <th>Chống ẩm</th>
        <th>Đơn giá</th>
        <th>Số lượng</th>
    </tr>
    {{#each planks}}
    <tr>
        <td>{{this.MaVan}}</td>
        <td>{{this.TenVan}}</td>
        <td>{{this.DoDay}}</td>
        <td>{{this.NhaCungCap}}</td>
        <td>{{this.ChongAm}}</td>
        <td>{{this.DonGia}}</td>
        <td>{{this.SoLuong}}</td>
    </tr>
    {{/each}}
</table>