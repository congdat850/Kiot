<link rel="stylesheet" href="/stylesheets/listWarehousePlank.css">
<div class="list-warehouse-plank">
    <h3 class="title-page">DANH SÁCH VÁN</h3>
     <div class="search-component">
         <cd-search for="warehousePlank" categorys={{this.categorys}}></cd-search>
    </div>
    <table class="table-list">
        <tr>
            <th>STT</th>
            <th>Mã ván</th>
            <th>Tên ván</th>
            <th>Độ dày</th>
            <th>Nhà cung cấp</th>
            <th>Chống ẩm</th>
            <th>Đơn giá</th>
            <th>Số lượng</th>
        </tr>
        {{#each planks}}
        <tr>
            <td>{{inc @index}}</td>
            <td>{{this.MaVan}}</td>
            <td>{{this.TenVan}}</td>
            <td>{{this.DoDay}}</td>
            <td>{{this.NhaCungCap}}</td>
            <td>{{this.ChongAm}}</td>
            <td>{{this.DonGia}}</td>
            <td>{{this.SoLuong}}</td>
        </tr>
        {{/each}}
    </table>
    <hd-paging current-page="{{page}}" max-page="{{maxPage}}"></hd-paging>
</div>